{
  "name": "prisma-github-auth",
  "version": "1.0.0",
  "scripts": {
    "start": "dotenv -- nodemon -x ts-node -e ts,graphql src/index.ts",
    "debug": "dotenv -- nodemon -x 'ts-node --inspect' -e ts,graphql src/index.ts",
    "playground": "dotenv -- graphql playground",
    "build": "rm -rf dist && tsc"
  },
  "dependencies": {
    "graphql": "0.12.3",
    "graphql-import": "0.4.1",
    "graphql-yoga": "1.2.0",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.1.1",
    "prisma-binding": "1.4.0"
  },
  "devDependencies": {
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.12.4",
    "nodemon": "1.14.11",
    "prisma": "1.0.7",
    "ts-node": "4.1.0",
    "typescript": "2.6.2"
  },
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
